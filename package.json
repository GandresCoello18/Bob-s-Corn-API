{
  "name": "bobs-corn-api",
  "version": "1.0.0",
  "description": "Clean architecture Node.js + TypeScript API for Bob's Corn",
  "main": "dist/index.js",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "cross-env APP_ENV=development nodemon",
    "dev:prod": "cross-env APP_ENV=production nodemon",
    "build": "tsc",
    "build:prod": "cross-env APP_ENV=production tsc",
    "start": "cross-env APP_ENV=production node dist/index.js",
    "start:dev": "cross-env APP_ENV=development node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "keywords": [
    "api",
    "typescript",
    "fastify",
    "clean-architecture"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^17.2.3",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^9.1.0",
    "pino-pretty": "^11.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
